@DatabaseTable(tableName = "ORDER")
public class OrderM implements Serializable {

    public static final String WAITUPLOAD = "wait_upload";
    public static final String WAITUPDATENUMBER = "wait_update_number";
    public static final String Over = "over";

    @DatabaseField(
            generatedId = true
    )
    private int id;

    @DatabaseField
    private String barCode;
    @DatabaseField
    private String picture;
    @DatabaseField
    private String status;
    @DatabaseField
    private String certNo;
    @DatabaseField
    private String createTime;
    @DatabaseField
    private String name;
    @DatabaseField
    private String parkName;
    @DatabaseField
    private String begin;
    @DatabaseField
    private String end;
    @DatabaseField
    private String subBarCode;
    @DatabaseField
    private String visitedId;
    @DatabaseField
    private String barCodeFlag;
    @DatabaseField
    private int position;

    public int getPosition() {
        return position;
    }

    public void setPosition(int position) {
        this.position = position;
    }

    public String getBarCodeFlag() {
        return barCodeFlag;
    }

    public void setBarCodeFlag(String barCodeFlag) {
        this.barCodeFlag = barCodeFlag;
    }

    public String getVisitedId() {
        return visitedId;
    }

    public void setVisitedId(String visitedId) {
        this.visitedId = visitedId;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getBarCode() {
        return barCode;
    }

    public void setBarCode(String barCode) {
        this.barCode = barCode;
    }

    public String getPicture() {
        return picture;
    }

    public void setPicture(String picture) {
        this.picture = picture;
    }

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getCertNo() {
        return certNo;
    }

    public void setCertNo(String certNo) {
        this.certNo = certNo;
    }

    public String getCreateTime() {
        return createTime;
    }

    public void setCreateTime(String createTime) {
        this.createTime = createTime;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getParkName() {
        return parkName;
    }

    public void setParkName(String parkName) {
        this.parkName = parkName;
    }

    public String getBegin() {
        return begin;
    }

    public void setBegin(String begin) {
        this.begin = begin;
    }

    public String getEnd() {
        return end;
    }

    public void setEnd(String end) {
        this.end = end;
    }

    public String getSubBarCode() {
        return subBarCode;
    }

    public void setSubBarCode(String subBarCode) {
        this.subBarCode = subBarCode;
    }

}
