##################### 每台设备的个性化配置 ##################
[TERMINF]
#商户号
MID=123456789111115
#终端号
TID=11100013
#商户名称
MCHTNAME=测试商户
#自动签到日期:修改可触发自动签到
LOGINDATE=20150702
#自动结算日期:无需修改
SETTLEDATE=20150702

#流水号:重复交易时修改
TRACENO=002223
#批次号：无需修改
BATCHNO=000000

#是否生成签购单:0-不生成，1-生成
PRINTRECEIPT=1
#校验序列号
AUTHSN=BC06F7C7372BA2A61E9D16DCD753CF2E2CE297F6959D58A72323D8BED30BE786q
#是否支持无密交易:0-不支持 1-支持
NoPinSupport=0


[PINPAD]
#加密算法设置 0：单倍长密钥 6：双倍长密钥算法
ENCRY_MODE=6
#PIN加密算法 1:不带主账号 2:带主账号信息
PIN_FORMAT=2
#送回车模式:1键盘自动送回车 2键盘需要输入回车 3键盘输六位后，我方补回车
EnterMode=1
#主密钥索引
MKeyIndex=1
#工作密钥索引
PinIndex=1
#MAC密钥索引
MacIndex=3

#密码长度
PinLength=6

#timeOut
PinPADTimeout=10

#声音开关 0关闭蜂鸣器 1打开蜂鸣器
EnablBeep=1

#Mac算法1-CBC 3-ECB
MacAlgorithm=1


